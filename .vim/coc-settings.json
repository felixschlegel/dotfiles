{
    "languageserver": {
        "ocaml-lsp": {
            "command": "opam",
            "args": ["config", "exec", "--", "ocamllsp"],
            "filetypes": ["ocaml", "reason"]
        },
        "sourcekit-lsp": {
          "filetypes": ["swift", "c", "cpp"],
          "command": "sourcekit-lsp"
        }
    },
    "coc.preferences.diagnostic.displayByAle": true,
    "suggest.noselect": false
}
